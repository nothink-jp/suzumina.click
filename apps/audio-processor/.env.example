# Cloud Run Jobs 音声処理環境変数設定例

# GCP プロジェクト設定
GOOGLE_CLOUD_PROJECT=suzumina-click-firebase
GOOGLE_CLOUD_REGION=us-central1

# Cloud Storage設定
AUDIO_BUCKET_NAME=suzumina-click-audio-files

# Firestore設定
FIRESTORE_DATABASE=(default)

# 音声処理設定
MAX_AUDIO_BUTTONS=20
MIN_SEGMENT_DURATION=1.0
MAX_SEGMENT_DURATION=10.0

# 音声品質設定
OPUS_BITRATE=128k
AAC_BITRATE=128k

# ログレベル
LOG_LEVEL=INFO

# Cloud Run Jobs設定
JOB_TIMEOUT=3600
PORT=8080

# Cloud Run Jobs環境フラグ
CLOUD_RUN_JOB=true